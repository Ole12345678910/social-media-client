{
  "name": "css-frameworks-ca",
  "version": "1.0.0",
  "description": "Test",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "start": "concurrently \"sass --watch src/scss:dist/css\" \"live-server --port=8080 --ignore=dist/css\"",
    "lint": "eslint .",
    "format": "prettier --log-level warn --write \"**/*.{js,css}\"",
    "posttest": "npm run format",
    "test": "jest",
    "prepare": "husky",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:test:run": "start-server-and-test start http://localhost:8080 cy:run",
    "cy:test:open": "start-server-and-test start http://localhost:8080 cy:open"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.css": [
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "@eslint/js": "^9.12.0",
    "babel-jest": "^29.7.0",
    "concurrently": "^9.0.1",
    "cypress": "^13.15.0",
    "eslint": "^9.12.0",
    "eslint-plugin-cypress": "^4.0.0",
    "eslint-plugin-jest": "^28.8.3",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "live-server": "^1.2.2",
    "mrm": "^4.1.22",
    "mrm-task-eslint": "^4.1.22",
    "mrm-task-lint-staged": "^7.1.22",
    "mrm-task-prettier": "^5.1.22",
    "prettier": "^3.3.3",
    "sass": "^1.80.2",
    "start-server-and-test": "^2.0.8"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  }
}
