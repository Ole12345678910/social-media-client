    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "live-server": "^1.2.2",
    "mrm": "^4.1.22",
    "mrm-task-eslint": "^4.1.22",
    "mrm-task-lint-staged": "^7.1.22",
